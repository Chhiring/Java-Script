//function to create inverted Left pyramid using given height
function invertedLeftPyramid(a){
  var pattern = '';
  for(var i=a; i>=1; i--){
    for(var j=a; j>=1; j--){
      if(j>i){
        pattern+= "  ";
      }
      else{
        pattern+= "* ";
      }
    }
    console.log(pattern);
    pattern='';
  }
}
//call invertedLeftPyramid function to print pyramid
invertedLeftPyramid(5);
