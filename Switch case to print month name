// function to print a month when user inputs a number
function printmonth(a) {
    switch (a) {
        case 1:
            month = "January";
            break;
        case 2:
            month = "Febraury";
            break;
        case 3:
            month = "March";
            break;
        case 4:
            month = "April";
            break;
        case 5:
            month = "May";
            break;
        case 6:
            month = "June";
            break;
        case 7:
            month = "July";
            break;
        case 8:
            month = "August";
            break;
        case 9:
            month = "September";
            break;
        case 10:
            month = "October";
            break;
        case 11:
            month = "November";
            break;
        case 12:
            month = "December";
            break;
        default:
            month = "INVALID NUMBER FOR MONTH";
    }
    console.log(month);
}
printmonth(12); // calling printmonth function with number 12
printmonth(15); // calling print month function with number 15
