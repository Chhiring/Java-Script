//function to create inverted right pyramid using given height
function invertedRightPyramid(a){
  var pattern = '';
  for(var i=a; i>=1; i--){
    for(var j=a; j>=1; j--){
      if(j>i){
        pattern+= "  ";
      }
      else{
        pattern+= "* ";
      }
    }
    console.log(pattern);
    pattern='';
  }
}
//call invertedRightPyramid function to print pyramid
invertedRightPyramid(5);
